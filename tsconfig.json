{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "outDir": "docs/js",
        "strictNullChecks": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "strictPropertyInitialization": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "alwaysStrict": true,
        "noImplicitThis": true,
    },
    "include": ["src/**/*"],
}
